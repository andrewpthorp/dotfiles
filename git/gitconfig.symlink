[user]
  name = Andrew Thorp
  email = andrewpthorp@gmail.com
[alias]
  stat = status
  co = checkout
  filenames = show --name-only
  addall = add -A
  pretty = log --oneline --first-parent
[github]
  user = andrewpthorp
[color]
  ui = auto
[web]
	browser = googlechrome
[browser "googlechrome"]
	cmd = "open -a /Applications/Google\\ Chrome.app"
[push]
	default = current
[pull]
  rebase = true
[init]
	templatedir = ~/.git_template
